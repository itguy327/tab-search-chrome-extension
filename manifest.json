{
  "name": "Tab Search",
  "version": "0.0.1",
  "description": "Have too many tabs? Search for text in the tab to find the one you want.",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "idle",
    "system.memory",
    "tabCapture",
    "management",
    "bookmarks",
    "history",
    "downloads",
    "sessions"
  ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}